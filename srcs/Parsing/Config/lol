server {
 	listen   		fasd;
	server_name  domain2.com www.domain2.com;
	access_log   logs/domain2.access.log  main;
	root	bruh;

	location {
		root    /var/www/virtual/big.server.com/htdocs;
		expires 30d;
	}

	location {
		root lol;
		root lel;
	}

	location derp derp {
		root asdas;
	}

	location / {
		proxy_pass      http://127.0.0.1:8080;
	}
}